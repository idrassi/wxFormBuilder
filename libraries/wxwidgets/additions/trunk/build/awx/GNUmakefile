../lib/libawx$(LIBPOSTFIX)-0.1.a: $(AWX_LIB_OBJECTS)
	rm -f $@
	$(AR) rcu $@ $(AWX_LIB_OBJECTS)
	$(RANLIB) $@

install_awx_lib: 
	$(INSTALL) -d $(prefix)/lib
	$(INSTALL) -m 644 ../lib/libawx$(LIBPOSTFIX)-0.1.a $(prefix)/lib

../samples/awxtest: $(AWXTEST_OBJECTS) ../lib/libawx$(LIBPOSTFIX)-0.1.a
	$(CXX) -o $@ $(AWXTEST_OBJECTS) $(LDFLAGS) -L../lib   $(WX_LIBS) ../lib/libawx$(LIBPOSTFIX)-0.1.a

gnu/awx_lib_button.o: ./../src/button.cpp
	$(CXX) -c -o $@ $(AWX_LIB_CXXFLAGS) $(CPPDEPS) $<

