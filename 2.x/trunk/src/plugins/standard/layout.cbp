			<Option target="Release" />
			<Option target="Debug (Unicode)" />
			<Option target="Release (Unicode)" />
		</Unit>
		<Unit filename="layout.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Release" />
			<Option target="Debug (Unicode)" />
			<Option target="Release (Unicode)" />
		</Unit>
		<Unit filename="..\..\utils\xrcconv.cpp">
			<Option compilerVar="CPP" />
			<Option target="Debug" />
			<Option target="Release" />
