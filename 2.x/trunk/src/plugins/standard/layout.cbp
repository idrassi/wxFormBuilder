<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="5" />
	<Project>
		<Option title="Layout Components Plugin" />
		<Option pch_mode="0" />
		<Option default_target="" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="..\..\..\bin\xml\liblayout.dll" />
				<Option object_output=".objsd" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG" />
					<Add directory="$(#WX.lib)\gcc_dll\mswd" />
				</Compiler>
				<Linker>
					<Add library="tinyxmld" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="d" />
				</Environment>
			</Target>
			<Target title="Debug (Unicode)">
				<Option output="..\..\..\bin\xml\liblayout.dll" />
				<Option object_output=".objsud" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="-O0" />
					<Add option="-D_DEBUG" />
					<Add option="-D__WXDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DUNICODE" />
				</Compiler>
				<Linker>
					<Add library="tinyxmld" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="ud" />
				</Environment>
			</Target>
			<Target title="Release">
				<Option output="..\..\..\bin\xml\liblayout.dll" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="tinyxml" />
				</Linker>
			</Target>
			<Target title="Release (Unicode)">
				<Option output="..\..\..\bin\xml\liblayout.dll" />
				<Option object_output=".objsu" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-Os" />
					<Add option="-O3" />
					<Add option="-DNDEBUG" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DUNICODE" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add library="tinyxml" />
				</Linker>
				<Environment>
					<Variable name="WX_SUFFIX" value="u" />
				</Environment>
			</Target>
			<Environment>
				<Variable name="WX_SUFFIX" value="" />
				<Variable name="WX_VER" value="27" />
				<Variable name="tinyxml" value="..\..\..\libs\tinyxml" />
			</Environment>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-DWIN32" />
			<Add option="-D_WINDOWS" />
			<Add option="-DHAVE_W32API_H" />
			<Add option="-DBUILD_DLL" />
			<Add option="-D__WX__" />
			<Add option="-D__WXMSW__" />
			<Add option="-DWXUSINGDLL" />
			<Add option="-DTIXML_USE_TICPP" />
			<Add directory="$(#WX.include)" />
			<Add directory="$(tinyxml)" />
			<Add directory="..\.." />
		</Compiler>
		<Linker>
			<Add library="wxmsw$(WX_VER)$(WX_SUFFIX)" />
			<Add directory="$(#WX.lib)\gcc_dll" />
			<Add directory="$(tinyxml)\lib" />
		</Linker>
		<Unit filename="..\component.h">
			<Option compilerVar="" />
			<Option compile="0" />
			<Option link="0" />
