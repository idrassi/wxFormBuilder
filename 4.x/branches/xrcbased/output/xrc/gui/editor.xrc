<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwidgets.org/wxxrc" version="2.5.3.0">
    <object class="wxNotebook" name="Editor">
        <object class="notebookpage">
            <bitmap>images/designer.png</bitmap>
            <label>Designer</label>
            <object class="wxScrolledWindow" name="VisualEditor">
                <style>wxHSCROLL|wxVSCROLL</style>
                <scrollrate>5,5</scrollrate>
                <bg>#c0c0c0</bg>
                <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <minsize>150,-1</minsize>
                    <object class="sizeritem">
                        <option>0</option>
                        <flag>wxLEFT|wxTOP</flag>
                        <border>9</border>
                        <object class="wxPanel" name="DesignerWindow">
                            <style>wxRAISED_BORDER|wxTAB_TRAVERSAL</style>
                            <object class="wxBoxSizer">
                                <orient>wxVERTICAL</orient>
                                <object class="sizeritem">
                                    <option>0</option>
                                    <flag>wxEXPAND</flag>
                                    <border>5</border>
                                    <object class="wxPanel" name="WindowTitle">
                                        <style>wxTAB_TRAVERSAL</style>
                                        <bg>#000000</bg>
                                        <object class="wxBoxSizer">
                                            <orient>wxHORIZONTAL</orient>
                                            <object class="sizeritem">
                                                <option>0</option>
                                                <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>3</border>
                                                <object class="wxStaticBitmap" name="BitmapTitle">
                                                    <bitmap>images/w_icon.png</bitmap>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <option>1</option>
                                                <flag>wxALIGN_CENTER_VERTICAL|wxALL</flag>
                                                <border>3</border>
                                                <object class="wxStaticText" name="WindowLabel">
                                                    <fg>#ffffff</fg>
                                                    <font>
                                                        <size>9</size>
                                                        <family>default</family>
                                                        <style>normal</style>
                                                        <weight>bold</weight>
                                                        <underlined>0</underlined>
                                                    </font>
                                                    <label>Title</label>
                                                    <wrap>-1</wrap>
                                                </object>
                                            </object>
                                            <object class="sizeritem">
                                                <option>0</option>
                                                <flag>wxALL</flag>
                                                <border>3</border>
                                                <object class="wxBoxSizer">
                                                    <orient>wxHORIZONTAL</orient>
                                                    <object class="sizeritem">
                                                        <option>0</option>
                                                        <flag></flag>
                                                        <border>3</border>
                                                        <object class="wxStaticBitmap" name="ButtonMinimize">
                                                            <bitmap>images/w_minimize.png</bitmap>
                                                        </object>
                                                    </object>
                                                    <object class="sizeritem">
                                                        <option>0</option>
                                                        <flag></flag>
                                                        <border>3</border>
                                                        <object class="wxStaticBitmap" name="ButtonMaximize">
                                                            <bitmap>images/w_maximize.png</bitmap>
                                                        </object>
                                                    </object>
                                                    <object class="sizeritem">
                                                        <option>0</option>
                                                        <flag></flag>
                                                        <border>3</border>
                                                        <object class="wxStaticBitmap" name="ButtonClose">
                                                            <bitmap>images/w_close.png</bitmap>
                                                        </object>
                                                    </object>
                                                </object>
                                            </object>
                                        </object>
                                    </object>
                                </object>
                                <object class="sizeritem">
                                    <option>1</option>
                                    <flag>wxEXPAND | wxALL</flag>
                                    <border>5</border>
                                    <object class="wxPanel" name="WindowPanel">
                                        <style>wxTAB_TRAVERSAL</style>
                                        <size>150,90</size>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
        <object class="notebookpage">
            <bitmap>images/file_cpp.png</bitmap>
            <label>C++</label>
            <selected>0</selected>
            <object class="wxPanel" name="CppPanel">
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                    <object class="sizeritem">
                        <option>1</option>
                        <flag>wxEXPAND | wxALL</flag>
                        <border>5</border>
                        <object class="wxNotebook" name="CppBook">
                            <object class="notebookpage">
                                <bitmap>images/file_cpp.png</bitmap>
                                <label>cpp</label>
                                <selected>0</selected>
                                <object class="wxStyledTextCtrl" name="CppPage">
                                    <font>
                                        <family>modern</family>
                                    </font>
                                    <line_numbers>1</line_numbers>
                                    <fold>1</fold>
                                    <indentation_guides>1</indentation_guides>
                                    <use_tabs>1</use_tabs>
                                    <tab_indents>1</tab_indents>
                                    <tab_width>4</tab_width>
                                    <backspace_unindents>1</backspace_unindents>
                                    <view_eol>0</view_eol>
                                    <view_whitespace>1</view_whitespace>
                                    <caret_width>2</caret_width>
                                    <lexer>5</lexer>
                                    <keywords>asm auto bool break case catch char class const const__cast continue default delete do double dynamic__cast else enum explicit export extern false float for friend goto if inline int long mutable namespace new operator private protected public register reinterpret__cast return short signed sizeof static static__cast struct switch template this throw true try typedef typeid typename union unsigned using virtual void volatile wchar__t while</keywords>
                                </object>
                            </object>
                            <object class="notebookpage">
                                <bitmap>images/file_h.png</bitmap>
                                <label>h</label>
                                <selected>0</selected>
                                <object class="wxPanel" name="HppPage">
                                    <style>wxTAB_TRAVERSAL</style>
                                    <object class="wxBoxSizer">
                                        <orient>wxVERTICAL</orient>
                                    </object>
                                </object>
                            </object>
                        </object>
                    </object>
                </object>
            </object>
        </object>
        <object class="notebookpage">
            <bitmap>images/file_py.png</bitmap>
            <label>Python</label>
            <selected>0</selected>
            <object class="wxPanel" name="PyPanel">
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                </object>
            </object>
        </object>
        <object class="notebookpage">
            <bitmap>images/file_xml.png</bitmap>
            <label>XRC</label>
            <selected>0</selected>
            <object class="wxPanel" name="XrcPanel">
                <style>wxTAB_TRAVERSAL</style>
                <object class="wxBoxSizer">
                    <orient>wxVERTICAL</orient>
                </object>
            </object>
        </object>
    </object>
</resource>
