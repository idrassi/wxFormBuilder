    <property name="enabled" type="bool">1</property>
    <property name="hidden" type="bool">0</property>
    <property name="tooltip" type="wxString" />
  </objectinfo>
  
  <objectinfo class="wxButton" icon="button.xpm" type="widget">
    <inherits class="wxWindow" />
    <property name="name" type="text" hidden="1">m_button</property>
